Proceso doblar_numero
	escribir " algoritmo doblar numero ";
	//paso 1 solicitar el numero 
	definir data como entero;
	escribir " ingresar el numero ";
	leer data;
	//paso 2 doblar el nuemero y mult por 25
	definir res como entero;
	res <- data*2*25;
	//;paso 3 dar a conocer el resultado
	escribir " el resultado es ",res;
FinProceso
